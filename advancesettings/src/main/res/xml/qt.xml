<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
   <Preference android:title="状态栏电量样式">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.fuelgauge.BatteryIndicatorStyle" />
   </Preference>
   <Preference android:title="运行模式">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.fuelgauge.PowerModeSettings" />
   </Preference>
   <Preference android:title="运行统计">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.UsageStatsActivity" />
   </Preference>
   <Preference android:title="极致省电">
      <intent
         android:targetPackage="com.miui.powerkeeper"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.powerkeeper.ui.ExtremePowerEntryActivity" />
   </Preference>
   <Preference android:title="自启管理">
      <intent
         android:targetPackage="com.miui.securitycenter"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.permcenter.autostart.AutoStartManagementActivity" />
   </Preference>
   <Preference android:title="续航管理图">
      <intent
         android:targetPackage="com.miui.securitycenter"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.powercenter.batteryhistory.BatteryHistoryDetailActivity" />
   </Preference>
   <Preference android:title="联网控制">
      <intent
         android:targetPackage="com.miui.securitycenter"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.networkassistant.ui.activity.FirewallActivity" />
   </Preference>
   <Preference android:title="卸载应用">
      <intent
         android:targetPackage="com.miui.cleanmaster"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.optimizecenter.deepclean.installedapp.InstalledAppsActivity" />
   </Preference>
   <Preference android:title="多用户">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.Settings$UserSettingsActivity" />
   </Preference>
   <Preference android:title="定时开关机">
      <intent
         android:targetPackage="com.miui.securitycenter"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.miui.powercenter.bootshutdown.PowerShutdownOnTime" />
   </Preference>
   <Preference android:title="权限管理">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.Settings$OverlaySettingsActivity" />
   </Preference>
   <Preference android:title="通知日志">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.Settings$NotificationStationActivity" />
   </Preference>
   <Preference android:title="息屏时间">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.display.ScreenTimeoutDialogActivity" />
   </Preference>
   <Preference android:title="原生内存管理">
      <intent
         android:targetPackage="com.android.settings"
         android:action="android.intent.action.MAIN"
         android:targetClass="com.android.settings.Settings$MemorySettingsActivity" />
   </Preference>
</PreferenceScreen>
